[('Onehot', OneHotEncoder(ignore_format=True,
              variables=['DriverNumber', 'Team', 'Compound', 'TyreLife',
                         'SpeedI1', 'SpeedI2', 'SpeedFL', 'SpeedST',
                         'MeanSpeed'])), ('Model', GridSearchCV(cv=3, estimator=GradientBoostingRegressor(random_state=42),
             param_grid={'learning_rate': [0.01, 0.1, 0.2],
                         'max_depth': [5, 10, 15],
                         'n_estimators': [100, 200, 500],
                         'subsample': [0.8, 1]},
             scoring='r2', verbose=2))]